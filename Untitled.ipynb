   "source": [
    "from scipy.stats import bernoulli\n",
    "from matplotlib import pyplot as plt\n",
    "from random import uniform\n",
    "\n",
    "p = uniform(0, 1)\n",
    "X = bernoulli(p)\n",
    "xs = range(2)\n",
    "\n",
    "plt.bar(xs, height=X.pmf(xs))\n",
    "plt.xticks([0,1])\n",
    "plt.show()"
   ]
